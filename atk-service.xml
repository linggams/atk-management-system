<service>
  <id>atk-app</id>
  <name>ATK App</name>
  <description>Next.js ATK app - port 2000</description>
  <executable>C:\Windows\System32\cmd.exe</executable>
  <!-- Sesuaikan path di bawah jika project ada di folder lain (mis. C:\next\atk) -->
  <arguments>/c "C:\atk\start-app.bat"</arguments>
  <workingdirectory>C:\atk</workingdirectory>
  <logpath>C:\atk\logs</logpath>
  <log mode="roll"></log>
</service>
